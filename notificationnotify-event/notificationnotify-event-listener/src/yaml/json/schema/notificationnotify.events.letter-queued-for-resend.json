{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://moj.gov.uk/cpp/json/schemas/notificationnotify/events/letter-queued-for-resend.json",
  "type": "object",
  "properties": {
    "notificationId": {
      "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
    },
    "letterUrl": {
      "type": "string",
      "format": "uri"
    },
    "reason": {
      "type": "string"
    },
    "postage": {
      "type": "string"
    }
  },
  "additionalProperties": false,
  "required": [
    "notificationId",
    "letterUrl",
    "reason"
  ]
}
