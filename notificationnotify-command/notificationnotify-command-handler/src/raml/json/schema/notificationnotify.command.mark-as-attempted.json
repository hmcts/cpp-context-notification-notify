{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://justice.gov.uk/json/schemas/domains/notificationnotify/notificationnotify.command.mark-as-attempted.json",
  "type": "object",
  "properties": {
    "notificationId": {
      "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
    },
    "attemptedTime": {
      "type": "string",
      "format": "date-time"
    },
    "errorMessage": {
      "type": "string"
    },
    "statusCode": {
      "type": "integer"
    }
  },
  "additionalProperties": false,
  "required": [
    "notificationId",
    "attemptedTime",
    "errorMessage",
    "statusCode"
  ]
}
