{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "notificationId": {
      "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
    },
    "status": {
      "type": "string"
    },
    "sendToAddress": {
      "type": "string"
    },
    "errorMessage": {
      "type": "string"
    },
    "statusCode": {
      "type": "integer"
    },
    "dateCreated": {
      "type": "string",
      "format": "date-time"
    },
    "lastUpdated": {
      "type": "string",
      "format": "date-time"
    },
    "materialUrl": {
      "type": "string"
    }
  },
  "required": [
    "notificationId",
    "status",
    "dateCreated"
  ]
}